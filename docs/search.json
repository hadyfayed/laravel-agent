---
layout: null
---
[
{% assign pages = site.html_pages | where_exp: "page", "page.title" %}
{% for page in pages %}
  {
    "title": {{ page.title | jsonify }},
    "url": "{{ page.url | relative_url }}",
    "description": {{ page.description | default: "" | jsonify }},
    "category": {{ page.nav_active | default: "" | jsonify }},
    "content": {{ page.content | strip_html | truncatewords: 100 | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
